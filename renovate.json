{
  "extends": [
    "config:base",
    "schedule:weekly"
  ],
  "rangeStrategy": "update-lockfile",
  "automerge": true,
  "labels": ["renovate"],
  "major": {
    "automerge": false
  },
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchPackageNames": ["aws-testing-library"],
      "schedule": null
    }
  ],
  "semanticCommits": true
}
