{
  "name": "aws-testing-library-examples",
  "version": "1.0.0",
  "description": "Usage examples for aws-testing-library",
  "main": "src/index.js",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest --maxWorkers=1",
    "test:ci": "JEST_JUNIT_OUTPUT_DIR=./reports/junit JEST_JUNIT_OUTPUT_NAME=${npm_package_name}-test-results.xml jest --runInBand --ci",
    "prettier": "prettier --write scripts/**/*.js src/**/*.ts",
    "prettier:ci": "prettier --list-different scripts/**/*.js src/**/*.ts",
    "deploy": "serverless deploy --stage dev",
    "remove": "serverless remove --stage dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erezrokah/aws-testing-library-examples.git"
  },
  "author": "Erez Rokah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erezrokah/aws-testing-library-examples/issues"
  },
  "devDependencies": {
    "@anttiviljami/serverless-stack-output": "^0.3.1",
    "@types/aws-lambda": "^8.10.39",
    "@types/chai": "^4.2.7",
    "@types/fs-extra": "^9.0.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.1",
    "@types/node-fetch": "^2.5.4",
    "@types/uuid": "^8.0.0",
    "aws-testing-library": "^1.0.9",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "fs-extra": "^9.0.0",
    "jest": "^26.0.1",
    "jest-junit": "^11.0.0",
    "prettier": "^2.0.0",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-s3-remover": "^0.6.0",
    "serverless-step-functions": "^2.15.0",
    "serverless-webpack": "^5.3.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.5.4",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.2",
    "webpack": "^4.41.5",
    "yargs": "^15.0.2"
  },
  "homepage": "https://github.com/erezrokah/aws-testing-library-examples#readme",
  "private": true,
