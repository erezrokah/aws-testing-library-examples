{
  "name": "aws-testing-library-examples",
  "version": "1.0.0",
  "description": "Usage examples for aws-testing-library",
  "main": "src/index.js",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:ci": "JEST_JUNIT_OUTPUT=./reports/junit/${npm_package_name}-test-results.xml jest --runInBand --ci",
    "prettier": "prettier --write scripts/**/*.js src/**/*.ts",
    "prettier:ci": "prettier --list-different scripts/**/*.js src/**/*.ts",
    "deploy": "serverless deploy --stage dev",
    "remove": "serverless remove --stage dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erezrokah/aws-testing-library-examples.git"
  },
  "author": "Erez Rokah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erezrokah/aws-testing-library-examples/issues"
  },
  "devDependencies": {
    "@anttiviljami/serverless-stack-output": "^0.3.1",
    "@types/aws-lambda": "^8.10.26",
    "@types/chai": "^4.1.7",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "@types/node-fetch": "^2.3.4",
    "@types/uuid": "^3.4.4",
    "aws-testing-library": "^1.0.1",
    "chai": "^4.2.0",
    "fs-extra": "^8.0.1",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "prettier": "^1.17.1",
    "serverless-iam-roles-per-function": "^2.0.1",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-pseudo-parameters": "^2.4.0",
    "serverless-s3-remover": "^0.6.0",
    "serverless-step-functions": "^1.21.1",
    "serverless-webpack": "^5.3.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2"
  },
  "homepage": "https://github.com/erezrokah/aws-testing-library-examples#readme",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.466.0",
    "node-fetch": "^2.6.0",
    "uuid": "^3.3.2"
  }
}
